# generate new chatroom id or use existing id
window.get_fb_chat_room_id = =>
    url_segments = document.location.href.split("/#")
    if url_segments[1]
      return url_segments[1]
    return _.sample(window.listOfAdjectives) + "-" + _.sample(window.listOfAnimals) + "-" + _.random(1, 1000)

window.get_memory_id = =>
    url_segments = document.location.href.split("/#&")
    return url_segments[1] if url_segments[1]
    return null


